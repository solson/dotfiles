{
  "coc.source.around.enable": false,
  "codeLens.enable": true,
  "deno.enable": true,
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextLines": 1,
  "diagnostic.warningSign": "",
  "diagnostic-languageserver.filetypes": { "sh": "shellcheck" },
  "git.addGBlameToBufferVar": true,
  "git.branchCharacter": "",
  "git.changedDecorator": "~",
  "git.untrackedDecorator": "?",
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.lens.implementations": false,
  "signature.target": "echo",
  "suggest.autoTrigger": "none",
  "suggest.enablePreselect": true,
  "suggest.noselect": false,
  "typescript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false
}
